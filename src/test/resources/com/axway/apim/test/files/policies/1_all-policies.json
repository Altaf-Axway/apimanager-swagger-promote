{
	"name": "${apiName}",
	"path": "${apiPath}",
	"status": "unpublished",
	"version": "1.0.0",
	"organization": {
		"development": "API Development ${orgNumber}",
		"target": [
			"Community",
			"Axway"
		]
	},
	"inboundProfiles": {
		"_default": {
			"securityProfile": "_default",
			"corsProfile": "_default",
			"monitorAPI": true,
			"monitorSubject": "authentication.subject.id"
		}
	},
	"outboundProfiles": {
		"_default": {
			"authenticationProfile": "_default",
			"routeType": "policy",
			"requestPolicy": null,
			"responsePolicy": null,
			"routePolicy": "${routePolicy}",
			"faultHandlerPolicy": null,
			"apiId": null,
			"apiMethodId": null,
			"parameters": [
			]
		}
	},
    "securityProfiles": [
     	{
     		"name": "_default",
     		"isDefault": true,
     		"devices": [
     			{
     				"name": "API Key",
     				"type": "apiKey",
     				"order": 0,
     				"properties": {
     					"apiKeyFieldName": "KeyId",
     					"takeFrom": "QUERY",
     					"removeCredentialsOnSuccess": "false"
     				}
     			}
     		]
     	}
     ]
}